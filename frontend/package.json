{
  "name": "codesage-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx --yes -p vite@5.0.8 -p @vitejs/plugin-react@4.2.1 -p tailwindcss@3.3.6 -p autoprefixer@10.4.16 -p postcss@8.4.32 vite",
    "build": "tsc && npx --yes -p vite@5.0.8 -p @vitejs/plugin-react@4.2.1 -p tailwindcss@3.3.6 -p autoprefixer@10.4.16 -p postcss@8.4.32 vite build",
    "preview": "npx --yes -p vite@5.0.8 vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "npx vitest@1.6.1 run --config vitest.config.js",
    "test:watch": "npx vitest@1.6.1 --config vitest.config.js",
    "test:ui": "npx vitest@1.6.1 --ui --config vitest.config.js",
    "test:coverage": "npx vitest@1.6.1 --coverage --config vitest.config.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.6.1"
  }
}
